<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Samples.SpecificCasesPage"
             Title="Specific Cases">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Loading Task to Alert" Command="{Binding LoadingTaskToAlert}" />
            <Button Text="Two Date Pickers" Command="{Binding TwoDatePickers}" />
            <Button Text="Start Loading Twice" Command="{Binding StartLoadingTwice}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>